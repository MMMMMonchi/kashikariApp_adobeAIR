package common {	import flash.display.Sprite;	import flash.text.TextField;	import MainContainer;	public class CreateTestSprite 	{				public function CreateTestSprite() 		{					}						public static function getTestRectangle(color:uint = 0xeeeeee, alpha:Number=1.0,width:int = 640, height:int = 960):Sprite {						var testSprite:Sprite = new Sprite();			testSprite.graphics.beginFill(color, alpha);			testSprite.graphics.drawRect(0, 0, width, height);			testSprite.graphics.endFill();						return testSprite;		}			}}